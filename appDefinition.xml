         <permissionProperties>{&quot;hashvariable&quot;:&quot;#appResource.user-account-login-icon.svg#&quot;,&quot;permission&quot;:{&quot;className&quot;:&quot;&quot;,&quot;properties&quot;:{}}}</permissionProperties>
      </appResource>
